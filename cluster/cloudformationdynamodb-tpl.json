{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
      "tableMusic": {
        "Type": "AWS::DynamoDB::Table",
        "Properties": {
          "TableName": "Music-ZZ-REG-ID",
          "AttributeDefinitions": [
            {
              "AttributeName": "music_id",
              "AttributeType": "S"
            }
          ],
          "KeySchema": [
            {
              "AttributeName": "music_id",
              "KeyType": "HASH"
            }
          ],
          "ProvisionedThroughput": {
            "ReadCapacityUnits": "5",
            "WriteCapacityUnits": "5"
          }
        }
      },
      "tablePlaylist": {
        "Type": "AWS::DynamoDB::Table",
        "Properties": {
          "TableName": "Playlist-ZZ-REG-ID",
          "AttributeDefinitions": [
            {
              "AttributeName": "PlaylistTitle",
              "AttributeType": "S"
            },
            {
              "AttributeName": "music_id",
              "AttributeType": "S"
            }
          ],
          "KeySchema": [
            {
              "AttributeName": "PlaylistTitle",
              "KeyType": "HASH"
            },
            {
              "AttributeName": "music_id",
              "KeyType": "RANGE"
            }
          ],
          "ProvisionedThroughput": {
            "ReadCapacityUnits": "5",
            "WriteCapacityUnits": "5"
          }
        }
      },
      "tableUsers": {
        "Type": "AWS::DynamoDB::Table",
        "Properties": {
          "TableName": "User-ZZ-REG-ID",
          "AttributeDefinitions": [
            {
              "AttributeName": "user_id",
              "AttributeType": "S"
            }
          ],
          "KeySchema": [
            {
              "AttributeName": "user_id",
              "KeyType": "HASH"
            }
          ],
          "ProvisionedThroughput": {
            "ReadCapacityUnits": "5",
            "WriteCapacityUnits": "5"
          }
        }
      }
    },
    "Description": "DynamoDB tables for ZZ-AWS-ACCESS-KEY-ID"
  }
